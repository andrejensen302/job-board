
<% @jobs.each do |job| %>
<div class="booyah-box">
  <h1><%= job.position %></h1><br />
  <i><%= job.address %></i><br />
  <i><%= job.company %></i><br />
  <i><%= job.city %></i><br />
  <i><%= job.state %></i><br />
  <i><%= job.zipcode %></i><br />
  <i><%= job.salary %></i><br />
</div>

<hr />
<% end %>
